{
  "name": "zustand-fetching",
  "version": "1.0.0",
  "private": false,
  "description": "Zustand fetching helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com:hecmatyar/zustand-fetching.git"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "dist/srs/index.ts",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:rollup",
    "build:rollup": "rollup --config",
    "build:ts": "del-cli dist && tsc --noEmit false",
    "check:lint": "eslint src/**/*.{ts,tsx}",
    "check:ts": "tsc --noEmit",
    "fix:lint": "eslint src/**/*.{ts,tsx} --fix",
    "fix:prettier": "prettier --write src/**/*.{json}",
    "publish:package": "del-cli tmp && clean-publish --temp-dir tmp",
    "sort:package": "npx sort-package-json",
    "test": "jest",
    "typeDoc": "npx typedoc src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-eslint": "^10.1.0",
    "clean-publish": "^3.4.5",
    "date-fns": "^2.28.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "rollup": "^2.51.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-smart-asset": "^2.1.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.7.4",
    "zustand": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "zustand": ">=3"
  },
  "engines": {
    "node": ">=17",
    "npm": ">=8"
  },
  "engineStrict": true,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
  }
}
