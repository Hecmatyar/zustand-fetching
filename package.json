{
  "name": "zustand-fetching",
  "version": "1.0.2",
  "private": false,
  "description": "Zustand fetching helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com:hecmatyar/zustand-fetching.git"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "dist/index.ts",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check:lint": "eslint src/**/*.{ts,tsx,js}",
    "build:ts": "del-cli dist && tsc --noEmit false",
    "check:ts": "tsc --noEmit",
    "fix:lint": "eslint src/**/*.{ts,tsx,js} --fix",
    "fix:prettier": "prettier --write src/**/*.{json}",
    "sort:package": "npx sort-package-json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": []
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "clean-publish": "^4.0.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "typescript": "^4.7.4",
    "zustand": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "zustand": ">=4"
  },
  "engines": {
    "node": ">=17",
    "npm": ">=8"
  },
  "engineStrict": true,
  "publishConfig": {
    "homepage": "https://github.com/Hecmatyar/zustand-fetching"
  },
  "license": "MIT",
  "keywords": [
    "zustand",
    "fetching",
    "state",
    "store",
    "helpers"
  ]
}
